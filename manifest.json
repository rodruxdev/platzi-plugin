{
  "manifest_version": 3,
  "name": "Platzi Time Calculator",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "description": "This plugin let you know the exact time of a course.",
  "default_locale": "es",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "author": "rodrigojgq@gmail.com",
  "permissions": ["activeTab", "scripting"]
}
